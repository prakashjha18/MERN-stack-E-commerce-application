  "bugs": {
    "url": "https://github.com/prakashjha18/MERN-stack-E-commerce-application/issues"
  },
  "homepage": "https://github.com/prakashjha18/MERN-stack-E-commerce-application#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
